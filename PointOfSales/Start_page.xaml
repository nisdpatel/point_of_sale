<Window x:Class="PointOfSales.Start_page"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PointOfSales"
        mc:Ignorable="d"
        Title="Start_page" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*">
                
            </RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            
        </Grid.RowDefinitions>
        <Button Name="user_btn" Content="User" Grid.Row="0" Click="user_btn_Click" />
        <Button Name="admin_btn" Content ="Administrator" Grid.Row="1" Click="admin_btn_Click" ></Button>

    </Grid>
</Window>
